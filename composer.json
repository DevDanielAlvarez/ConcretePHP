{
    "name": "alvarez/concrete-php",
    "description": "Generator of DTOs and Services for Laravel using stubs",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Alvarez\\ConcretePhp\\": "src/",
            "Alvarez\\Generators\\": "src/Generators/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "daniel",
            "email": "skyrimdaniel11@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "pestphp/pest": "4.x-dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alvarez\\ConcretePhp\\ConcreteServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}