{
    "name": "alvarez/concrete-php",
    "description": "Generator of DTOs and Services for Laravel using stubs",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Alvarez\\ConcretePhp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "daniel",
            "email": "skyrimdaniel11@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "pestphp/pest": "4.x-dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "illuminate/database": "^10.0|^11.0|^12.0"
    }
}